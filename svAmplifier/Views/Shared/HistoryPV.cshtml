@model UserIndexLayoutVM

@foreach (var item in Model.Picks)
{
    <div class="historyItem" id="@item.UserId">
        <div class="thumbnail">
                <div id="map-@(item.Id)" class="map" lat="@item.Latitude" lng="@item.Longitude"></div>
        </div>
        <div class="historyInfo">
            <p>Svamp: @item.MushroomName</p>
            <p>Datum: @item.DatePicked.ToString().Split(' ')[0]</p>
            <p>Vikt: @item.WeightInGrams g</p>
            <p>@item.Description</p>
        </div>
    </div>
}